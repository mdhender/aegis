msgstr	"thân: $message"

#
# This error message is issued when a user from the /etc/passwd file, or
# equivalent, cannot be found by number.
#
#       $Number         The offending user number.
#
msgid	"uid $number unknown"
msgstr	"uid $number không rõ"

